{
    "name": "mileschou/laradra",
    "description": "Package for Laravel with Hydra",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ory/hydra-sdk": "^1.1"
    },
    "require-dev": {
        "laravel/framework": "^6.20 | ^7.6 | ^8",
        "phpunit/phpunit": "^8.5 | ^9",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app",
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
